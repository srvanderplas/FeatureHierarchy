  for(j in 1:nrow(plot.parms)){
    p = gen.plot(df, get.aes(plot.parms[j,]), get.stats(plot.parms[j,]))
    ggsave(plot = p, 
           filename = sprintf("figure/lineupex/set-%d-plot-%d.pdf", i, j), 
           width=6, height=6, units="in", dpi=150)
    print(p)
  }
})
